$('#sellerModal').modal('hide')

<% if exist_folder_user %>
  Swal.fire({
    title: 'Este usuario ya es un responsable de esta venta',
    type: 'warning',
    confirmButtonText: 'Continuar',
    customClass: {
        confirmButton: 'btn btn-success',
        cancelButton: 'btn btn-danger'
    },
    buttonsStyling: false
  });
<% elsif @created_folder_user%>
  Swal.fire({
    title: 'Â¡Responsable agregado!',
    type: 'success',
    confirmButtonText: 'Continuar',
    customClass: {
        confirmButton: 'btn btn-success',
        cancelButton: 'btn btn-danger'
    },
    buttonsStyling: false
  });
<% else %>
  Swal.fire({
    title: 'Hubo un error al intentar agregar este responsable',
