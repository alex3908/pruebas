
<% @folder.clients.each do |client| %>
  <%= render partial: "folders/files/new_format/partials/purchase_conditions_client", locals: {
    client: client,
    original: true
  } %>

  <% if @with_copy %>
  <%= render partial: "folders/files/new_format/partials/purchase_conditions_client", locals: {
    client: client,
    original: false
  } %>
  <% end %>
<% end %>
