<%= form.fields_for property_name, OpenStruct.new(properties) do |p| %>
  <div class="row">
    <% digital_signature_service.creator.get_fields.each do |field| %>
      <div class="form-group col-md-6">
        <%= p.label field[:field_name].to_sym, I18n.t("digital_signature_services.#{digital_signature_service.name}.#{property_name}.#{field[:field_name]}") %>
        <%= p.text_field field[:field_name].to_sym, class: "form-control", type:field[:type] %>
      </div>
    <% end %>
  </div>
<% end %>
