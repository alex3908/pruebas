<div class="row">
  <div class="col-sm-12 col-md-6">
    <h1 class="banner-title"><%= name %></h1>
  </div>
  <div class="col-sm-12 col-md-6 flex-middle-right">
    <ol class="breadcrumb ol-projects">
      <% links.each do |link| %>
      <li class="breadcrumb-item text-capitalize"><%= link_to link[:name], link[:path] %></li>
      <% end %>
      <li class="breadcrumb-item text-capitalize active" aria-current="page"><%= last %></li>
    </ol>
  </div>
</div>
